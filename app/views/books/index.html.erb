<h1>Books</h1>
<% @book.each do |book| %>
<p>title</p> <p>body</p>
<span><%= book.title%> </span> <span><%= book.body %></span> 
<% end %>
<%= form_with model:@book, local:true do |f| %>
  <h2>New book</h2>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
  <%= link_to "Show","/books/:id"  %>
<% end %>